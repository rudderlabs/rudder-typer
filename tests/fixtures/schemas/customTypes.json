{
  "title": "Custom Types Fixture",
  "description": "This fixture validates generation for custom types.",
  "type": "object",
  "$defs": {
    "ct-boo": {
      "enum": [true, false],
      "type": ["boolean"]
    },
    "ct-string-enum": {
      "type": "string",
      "enum": ["type1", "type2", "type3"]
    },
    "ct-number-enum": {
      "type": "number",
      "enum": [1, 2, 3]
    },
    "ct-object": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "value": {
          "type": "number"
        }
      },
      "required": ["name"]
    },
    "ct-array": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "ct-nested": {
      "type": "object",
      "properties": {
        "obj": {
          "$ref": "#/$defs/ct-object"
        },
        "enum": {
          "$ref": "#/$defs/ct-string-enum"
        }
      }
    }
  },
  "properties": {
    "booleanEnum": {
      "$ref": "#/$defs/ct-boo"
    },
    "stringEnum": {
      "$ref": "#/$defs/ct-string-enum"
    },
    "numberEnum": {
      "$ref": "#/$defs/ct-number-enum"
    },
    "objectType": {
      "$ref": "#/$defs/ct-object"
    },
    "arrayType": {
      "$ref": "#/$defs/ct-array"
    },
    "nestedType": {
      "$ref": "#/$defs/ct-nested"
    }
  }
}
